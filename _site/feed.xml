		&lt;span class=&quot;c1&quot;&gt;//checking what the value of iscursorshowing is&lt;/span&gt;
		&lt;span class=&quot;n&quot;&gt;Debug&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;.&lt;/span&gt;&lt;span class=&quot;n&quot;&gt;Log&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;(&lt;/span&gt;&lt;span class=&quot;s&quot;&gt;&amp;quot;isCursorSHowing on Awake = &amp;quot;&lt;/span&gt; &lt;span class=&quot;p&quot;&gt;+&lt;/span&gt; &lt;span class=&quot;n&quot;&gt;isCursorShowing&lt;/span&gt;&lt;span class=&quot;p&quot;&gt;);&lt;/span&gt;
